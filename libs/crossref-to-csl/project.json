{
  "root": "libs/crossref-to-csl",
  "sourceRoot": "libs/crossref-to-csl/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/crossref-to-csl/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/crossref-to-csl"],
      "options": {
        "jestConfig": "libs/crossref-to-csl/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/crossref-to-csl",
        "tsConfig": "libs/crossref-to-csl/tsconfig.lib.json",
        "packageJson": "libs/crossref-to-csl/package.json",
        "main": "libs/crossref-to-csl/src/index.ts",
        "assets": ["libs/crossref-to-csl/*.md"]
      }
    }
  },
  "tags": []
}
