{
  "root": "libs/docx-jats",
  "sourceRoot": "libs/docx-jats/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/docx-jats",
        "tsConfig": "libs/docx-jats/tsconfig.lib.json",
        "packageJson": "libs/docx-jats/package.json",
        "main": "libs/docx-jats/src/index.ts",
        "assets": ["libs/docx-jats/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/docx-jats/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/docx-jats"],
      "options": {
        "jestConfig": "libs/docx-jats/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
