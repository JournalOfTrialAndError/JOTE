{
  "root": "libs/csl-consolidate",
  "sourceRoot": "libs/csl-consolidate/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/csl-consolidate/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/csl-consolidate"],
      "options": {
        "jestConfig": "libs/csl-consolidate/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/csl-consolidate",
        "tsConfig": "libs/csl-consolidate/tsconfig.lib.json",
        "packageJson": "libs/csl-consolidate/package.json",
        "main": "libs/csl-consolidate/src/index.ts",
        "assets": ["libs/csl-consolidate/*.md"]
      }
    }
  },
  "tags": []
}
