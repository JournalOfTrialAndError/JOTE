{
  "root": "libs/parse-text-cite",
  "sourceRoot": "libs/parse-text-cite/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/parse-text-cite/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/parse-text-cite"],
      "options": {
        "jestConfig": "libs/parse-text-cite/jest.config.js",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/parse-text-cite",
        "tsConfig": "libs/parse-text-cite/tsconfig.lib.json",
        "packageJson": "libs/parse-text-cite/package.json",
        "main": "libs/parse-text-cite/src/index.ts",
        "assets": ["libs/parse-text-cite/*.md"]
      }
    }
  },
  "tags": []
}
