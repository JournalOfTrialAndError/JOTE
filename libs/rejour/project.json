{
  "root": "libs/rejour",
  "sourceRoot": "libs/rejour/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/rejour",
        "tsConfig": "libs/rejour/tsconfig.lib.json",
        "packageJson": "libs/rejour/package.json",
        "main": "libs/rejour/src/index.ts",
        "assets": ["libs/rejour/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/rejour/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/rejour"],
      "options": {
        "jestConfig": "libs/rejour/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
