{
  "name": "jote",
  "version": "0.0.1",
  "license": "GPL-3.0+",
  "scripts": {
    "start": "nx serve",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "clean-libs": "echo 'for i in libs/relatex/* libs/rejour/* \\n do \\n rm -f $i/yarn.lock && rm -rf $i/node_modules && yarn --cwd $i \\n done' | bash",
    "update-readme": "ts-node -P ./libs/utils/tsconfig.lib.json ./libs/utils/src/lib/updateReadme.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "docx4js": "^3.2.20",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.4.6",
    "@nrwl/eslint-plugin-nx": "13.4.6",
    "@nrwl/jest": "13.4.6",
    "@nrwl/js": "13.4.6",
    "@nrwl/linter": "13.4.6",
    "@nrwl/nest": "13.4.6",
    "@nrwl/node": "13.4.6",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/tao": "13.4.6",
    "@nrwl/workspace": "13.4.6",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "@typescript/analyze-trace": "^0.6.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "jest-esm-transformer": "^1.0.0",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4",
    "commander": "^9.0.0",
    "remark": "^14.0.2",
    "remark-license": "^6.1.0",
    "remark-toc": "^8.0.1",
    "remark-usage": "^10.0.1",
    "to-vfile": "^7.2.3"
  },
  "packageManager": "yarn@1.22.1",
  "wallaby": {
    "autoDetect": true,
    "env": {
      "params": {
        "runner": "--experimental-vm-modules"
      }
    }
  },
  "repository": "https://github.com/journaloftrialanderror/jote",
  "author": "Thomas F. K. Jorna <hello@tefkah.com>",
  "description": "",
  "keywords": [
    "unified",
    "abstract",
    "syntax",
    "tree",
    "ast"
  ]
}
